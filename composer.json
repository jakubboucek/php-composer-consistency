{
    "name": "jakubboucek/composer-consistency",
    "description": "Checks if the vendor/ directory is consistent with project's composer.lock",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub BoucÌŒek",
            "email": "pan@jakubboucek.cz"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "JakubBoucek\\ComposerConsistency\\": "src/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.10"
    },
    "replace": {
        "jakubboucek/composer-vendor-checker": "self.version"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
